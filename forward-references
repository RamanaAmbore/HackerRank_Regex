# HakcerRank challenge problem link: https://www.hackerrank.com/challenges/forward-references/problem

#This is Java code. Python option is not available in HackerRank

import java.io.*;
import java.util.*;
import java.util.regex.*;  

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        while(sc.hasNextLine()){
            String tkn=sc.nextLine();
            boolean b2=Pattern.compile("^tac(tac(tic)?)*$").matcher(tkn).matches();  
            System.out.println(b2);
        }
    }
}
